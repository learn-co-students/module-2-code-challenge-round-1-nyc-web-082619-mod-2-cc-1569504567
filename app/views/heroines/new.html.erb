<h1>Create Your Heroine</h1>

<%= form_for @heroine, url: {action: "create"} do |h| %>
  <%= h.label :name, 'Secret Identity' %>
  <%= h.text_field :name %>
  <%= h.label :super_name, 'Heroine Name' %>
  <%= h.text_field :super_name %>
  <%= h.label :power_id, 'Power' %>
  <%= h.collection_select(:power_id, Power.all, :id, :name)%>
  <%= h.submit "Create" %>
<% end %>
