<h2>All Heroines</h2>
<h4>Search by Super Power</h4>
<%= form_tag heroines_path, method: :get do%>
  <%= text_field_tag :search, params[:search]%>
  <%= submit_tag :search%>
<%end%>
<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine_path(heroine) %> -
      <%= link_to :delete, heroine_path(heroine), method: :delete%>
    </li>
    <br>
  <% end %>
</ul>
<%= link_to "Create Heroine", new_heroine_path %>
